section.section.nimish-list(setBackground="../images/producttion/bg.png")
	.container
		.filter-catelog 
			h1.title.title_small.title_small-black Danh mục thẻ
			.form-group 
				select.form-control
					option(value="") Tất cả thẻ
			.tag-list.tabs
				.nav-list
					ul.tab-list
						li.active(rel='tab-1')
							p Cashback-hoàn tiền
						li(rel='tab-2')
							p Du lịch
						li(rel='tab-3')
							p Trả góp
						li(rel='tab-4')
							p Mua sắm
						li(rel='tab-5')
							p hashtag
						li(rel='tab-6')
							p hashtag
				#tab-1.panel.hashtag-content.active
					-for(i=1; i<10; i++)
